{
    "name": "zhengcai/encrypt",
    "description": "A simple Laravel SM4 encryption and authentication class.",
    "type": "library",
    "keywords": ["laravel", "api", "sign", "verify", "encrypt","SM4"],
    "homepage": "https://github.com/Jeses/encrypt.git",
    "require": {
        "php": "^7.3|^8.0",
        "firebase/php-jwt": "^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "zhengCai",
            "email": "yz_luck@163.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zhengcai\\Encrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhengcai\\Encrypt\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Zhengcai\\Encrypt\\EncryptServiceProvider"
            ],
            "aliases": {
                "Encrypt": "Zhengcai\\Encrypt\\Facades\\Encrypt"
            }
        }
    }
}
